{
  "$schema": "../../node_modules/ng-packagr/package.schema.json",
  "name": "@angular/library",
  "version": "0.0.1",
  "peerDependencies": {
    "@angular/common": "^9.1.4",
    "@angular/core": "^9.1.4",
    "tslib": "^1.10.0"
  },
  "dependencies": {
    "@angular/material-moment-adapter": "^9.2.0",
    "angular-resize-element": "^1.0.17",
    "@angular/material": "^9.2.3",
    "@angular/cdk": "^9.2.3",
    "ngx-color": "^4.1.0",
    "hammerjs": "^2.0.8",
    "moment": "^2.24.0"
  },
  "ngPackage": {
    "whitelistedNonPeerDependencies": [
      "@angular/material-moment-adapter",
      "angular-resize-element",
      "@angular/material",
      "@angular/cdk",
      "ngx-color",
      "hammerjs",
      "moment"
    ],
    "assets": [
      "CHANGELOG.md",
      "../library/assets/images/*.png",
      "../library/assets/fonts/*.ttf",
      "../library/assets/scss/*.scss"
    ],
    "lib": {
      "entryFile": "./src/public-api.ts",
      "cssUrl": "inline"
    },
    "dest": "../../dist/angular-library"
  }
}
